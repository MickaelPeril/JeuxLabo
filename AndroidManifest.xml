<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jeuxlabo"
    android:versionCode="1"
    android:versionName="1.0" >
    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="17" />
   <uses-permission android:name="android.permission.WAKE_LOCK" />
   <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"/>
   <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>
    <application
        android:allowBackup="true"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.jeuxlabo.MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER"
                     />
            </intent-filter>
        </activity>
         <activity android:name="com.example.jeuxlabo.MoveActivity" 
              android:screenOrientation="portrait"/>
        <activity android:name="com.example.jeuxlabo.Start" />
        <activity
            android:name="com.example.jeuxlabo.SettingsActivity"
            android:label="@string/title_activity_settings" >
        </activity>
        
    </application>

</manifest>